{
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "target": "es5",
        "jsx": "react"
    },
    "include": ["./src/**/*.ts", "./src/**/*.tsx"]
}
